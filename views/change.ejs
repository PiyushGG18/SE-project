    <div class="heading heading-secondary">Update Details</div>



    <div class="main_c">
      <div class="sec_reg">
        <div class="row">
          <div class="center">
            <div class="frm">
              <form action="/change" method="post">
                <div class="f_e" style="display: none;">
                  <input type="text" class="f_input2 f_r" id="u_username" placeholder="Username" name="userName" value="<%=currUser.username%>"/>
                </div>
                <div class="reg_cl">
                  <h3 class="reg">basic Info</h3>
                </div>
                <div>
                  <div id="section1">
                    <div class="f_e">
                      <input
                        type="text"
                        class="f_input"
                        id="u_name"
                        value="<%=currUser.basicInfo.name%>"
                        name="name"
                      />
                      <!-- <label for="u_name" class="f_label">Name</label> -->
                    </div>

                    <div class="f_e">
                      <!-- <label for="u_name" class="f_label">Name</label> -->
                      <input
                        value="<%= currUser.basicInfo.birthdate.toLocaleDateString('en-US', { day: '2-digit', month: 'long', year: 'numeric' }) %>"                    
                        type="text"
                        onfocus="(this.type = 'date')"
                        class="f_input"
                        id="u_dob"
                        name="birthdate"
                      />
                    </div>

                    <div class="f_e">
                      <input
                        type="email"
                        class="f_input"
                        id="u_email"
                        value="<%= currUser.basicInfo.email%>"
                        name="email"
                      />
                    </div>

                    <div class="f_e">
                      <input
                        type="text"
                        class="f_input"
                        id="u_phone"
                        value="<%= currUser.basicInfo.phoneNo%>"
                        name="phoneNo"
                      />
                    </div>
                    <div class="f_e">
                      <select
                        class="f_input"
                        id="u_gender"
                        name="gender"
                      >
                        <option value="" disabled selected hidden>
                          Gender
                        </option>
                        <% if (currUser.basicInfo.gender === 'Male') { %>
                          <option selected="selected">Male</option>
                        <% } else { %>
                          <option>Male</option>
                        <% } %>
                        <% if (currUser.basicInfo.gender === 'Female') { %>
                          <option selected="selected">Female</option>
                        <% } else { %>
                          <option>Female</option>
                        <% } %>                        
                      </select>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>

